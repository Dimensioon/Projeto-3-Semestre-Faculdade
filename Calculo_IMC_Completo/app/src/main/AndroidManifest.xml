<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projeto_trabalho_real">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DarkInformation" />
        <activity android:name=".DarkMasculino" />
        <activity android:name=".DarkFeminino" />
        <activity android:name=".LightFeminino" />
        <activity android:name=".LightInformation" />
        <activity android:name=".LightMasculino" />
    </application>

</manifest>